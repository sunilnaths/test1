pipeline {
    agent any
    
    stages {
        stage("Hello") {
	   environment {
		My_Username = credentials ("admin-user")
	  }
           steps {
                sh "echo 'username: $My_Username_USR'"
		sh "echo 'password: $My_Username_PSW'"
		sh "touch username: $My_Username_USR/default.yaml"
		sh "touch password: $My_Username_PSW/default.yaml"
            }
        }
    }
}
